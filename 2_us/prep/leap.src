source leaprc.protein.ff19SB
source leaprc.water.tip3p
loadamberparams frcmod.ionsjc_tip3p

Mol = loadpdb 1uao.pdb
check Mol
savepdb Mol GB.pdb
saveamberparm Mol GB.parm7 GB.rst7

solvatebox Mol TIP3PBOX 14.0 1.0

addions Mol Na+ 0
addions Mol Cl- 0

savepdb Mol wat.pdb
saveamberparm Mol wat.parm7 wat.rst7
quit
